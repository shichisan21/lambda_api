generator client {
  provider = "prisma-client-js"
 binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String @db.VarChar(255)
  price String @db.VarChar(255)
  genre String @db.VarChar(255)
}
